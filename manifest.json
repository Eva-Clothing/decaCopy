{
  "name": "decaCopy",
  "version": "0.01",
  "description": "Copy and drag paste upto 10 text items at a time",

  "manifest_version": 2,

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",

  "permissions": [
    "activeTab",
    "storage",
    "clipboardRead",
    "clipboardWrite",
    "storage"
  ]
}
